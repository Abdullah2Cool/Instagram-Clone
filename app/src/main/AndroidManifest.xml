<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hafiz.instagramclone">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.act_Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Likes.act_Likes"></activity>
        <activity android:name=".Profile.act_Profile"></activity>
        <activity android:name=".Search.act_Search"></activity>
        <activity android:name=".Share.act_Share"></activity>
        <activity android:name=".Profile.act_AccountSettings"></activity>
        <activity android:name=".Login.act_Login"></activity>
        <activity android:name=".Login.act_Register"></activity>
    </application>

</manifest>